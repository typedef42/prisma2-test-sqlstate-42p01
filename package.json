{
  "name": "prisma2-test-sqlerror",
  "version": "0.0.1",
  "license": "MIT",
  "description": "prisma2-test-sqlerror",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "start": "dotenv nest start",
    "start:dev": "dotenv nest start --watch",
    "start:debug": "dotenv nest start --debug --watch",
    "start:prod": "dotenv node dist/main",
    "test": "dotenv jest",
    "test:watch": "dotenv jest --watch",
    "test:cov": "dotenv jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "db:dev": "dotenv prisma2 dev",
    "db:generate": "dotenv prisma2 generate",
    "db:seed": "dotenv ts-node prisma/seed",
    "db:up": "docker-compose up --build -d",
    "db:down": "docker-compose down"
  },
  "keywords": [
    "nest"
  ],
  "author": "Yannis Torres",
  "devDependencies": {
    "@gojob/eslint-config": "^1.0.10",
    "@nestjs/cli": "^6.9.0",
    "@nestjs/schematics": "^6.7.0",
    "@nestjs/testing": "^6.7.1",
    "@playlyfe/gql": "^2.6.1",
    "@types/bluebird": "^3.5.24",
    "@types/express": "^4.17.2",
    "@types/graphql": "^14.5.0",
    "@types/jest": "^24.0.18",
    "@types/lodash": "^4.14.121",
    "@types/node": "^12.7.5",
    "@types/supertest": "^2.0.8",
    "babel-eslint": "^10.0.1",
    "dotenv-cli": "^3.1.0",
    "eslint": "^6.7.2",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-security": "^1.4.0",
    "jest": "^24.9.0",
    "jest-cli": "^24.1.0",
    "jest-environment-node": "^24.9.0",
    "nanoid": "^3.1.31",
    "nodemon": "^2.0.2",
    "pg": "^7.14.0",
    "prettier": "^1.18.2",
    "prisma2": "^2.0.0-preview018.2",
    "supertest": "^4.0.2",
    "ts-jest": "^24.1.0",
    "ts-loader": "^6.1.1",
    "ts-node": "^8.4.1",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^5.20.0",
    "typescript": "^3.6.3"
  },
  "dependencies": {
    "@nestjs/common": "^6.7.2",
    "@nestjs/core": "^6.7.2",
    "@nestjs/graphql": "^6.5.3",
    "@nestjs/platform-express": "^6.7.2",
    "@prisma/photon": "^2.0.0-preview018.2",
    "apollo-server-express": "^2.9.13",
    "class-validator": "^0.11.0",
    "graphql": "^14.5.8",
    "graphql-tools": "^4.0.6",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.0",
    "rxjs": "^6.5.3",
    "type-graphql": "^0.17.5"
  }
